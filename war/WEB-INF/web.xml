<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="4">
	<servlet>
		<servlet-name>Installation</servlet-name>
		<servlet-class>com.oriborohov.javacourse.InstallationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Installation</servlet-name>
		<url-pattern>/installation</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Calculate</servlet-name>
		<servlet-class>com.oriborohov.javacourse.Calculate</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Calculate</servlet-name>
		<url-pattern>/Calculate</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>StockDetailsServlet</servlet-name>
		<servlet-class>com.oriborohov.javacourse.StockDetailsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StockDetailsServlet</servlet-name>
		<url-pattern>/StockDetails</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
